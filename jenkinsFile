pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World from github'
            }
        }
        stage('Build') {
            steps {
                echo 'Building in github'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing in github'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying in github'
            }
        }
    }
}
